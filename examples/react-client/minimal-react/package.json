  "devDependencies": {
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-checker": "^0.11.0"
  }
}
