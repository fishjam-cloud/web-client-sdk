{
  "name": "@fishjam-example/minimal-react-example",
  "private": true,
  "version": "0.10.2",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "tsc --noEmit --watch",
