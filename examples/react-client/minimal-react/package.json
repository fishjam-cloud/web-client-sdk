{
  "name": "@fishjam-dev/minimal-react-example",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "watch": "tsc --noEmit --watch",
    "preview": "vite preview",
