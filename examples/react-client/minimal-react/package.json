    "lint:check": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@fishjam-dev/react-client": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "typescript": "^5.4.0",
    "vite": "^5.1.2",
    "vite-plugin-checker": "^0.6.4"
  }
}
